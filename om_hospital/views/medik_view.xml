<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="subjektif_view_form" model="ir.ui.view">
        <field name="name">subjektif.view.form</field>
        <field name="model">subjektif</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <notebook>
                        <page name="Subjektif" string="Subjektif">
                            <group>
                                <field name="perawat_id"/>
                                <field name="subjektif"/>
                                <field name="subjektif_dokter"/>
                                <field name="riwayat_penyakit"/>
                                <field name="riwayat_personal"/>
                                <field name="alergi"/>
                                <field name="riwayat_keluarga"/>
                            </group>
                        </page>
                        <page name="Objektif" string="Objektif">
                                <field name="subjektif"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="subjektif_action" model="ir.actions.act_window">
        <field name="name">Subjektif</field>
        <field name="res_model">subjektif</field>
        <field name="view_mode">form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Your first subjektif!
            </p>
        </field>
    </record>

    <menuitem id="hospiatl_medik_root_menu" parent="hospital_root_menu" name="Medik"/>
    <menuitem id="Subjectif_menu" name="Subjektif" parent="hospiatl_medik_root_menu" action="subjektif_action"/>

</odoo>